<style type="text/css">
/* http://www.gnostice.com/nl_article.asp?id=208&t=making_an_interactive_treeview_control_from_an_html_nested_list
*/
  NB.b {background-color:#fdb;}
  i {background-color:#dfd;}
  ul.LinkedList { display: block; }
  ul.LinkedList ul { display: none; }
  .hcStyle { cursor: pointer }  /* For IE */
  .leaf {	background-color:#fdd;
				cursor: help }  /* For IE */
  .doit {	background-color:#dff;
				cursor: cell }  /* For IE */
</style>
<script type="text/JavaScript">
// Add this to the onload event of the BODY element
function addEvents() { activateTree(
	document.getElementById("LinkedList1")); }
// This function traverses the list and add links to nested list items
function activateTree(oList) {
// Collapse the tree
for (var i=0; i < oList.getElementsByTagName("ul").length; i++) {
	oList.getElementsByTagName("ul")[i].style.display="none";
}
// Add the click-event handler to the list items
if (oList.addEventListener) {
	oList.addEventListener("click", toggleBranch, false);
} else if (oList.attachEvent) { // For IE
	oList.attachEvent("onclick", toggleBranch);
}
if (oList.addEventListener) {
	oList.addEventListener("dblclick", doAll, false);
} else if (oList.attachEvent) { // For IE
	oList.attachEvent("dblclick", doAll);
}
// Make the nested items look like links
addLinksToBranches(oList);
}
// This is the click-event handler
function toggleBranch(event) {
	var oBranch, cSubBranches;
	if (event.target) {
		oBranch = event.target;
	} else if (event.srcElement) { // For IE
		oBranch = event.srcElement;
	}
	cSubBranches = oBranch.getElementsByTagName("ul");
	if (cSubBranches.length > 0) {
			if (cSubBranches[0].style.display == "block") { cSubBranches[0].style.display = "none";
			} else { cSubBranches[0].style.display = "block"; }
	}
	if (oBranch.className =="leaf") 
		oBranch.className ="doit"
	else if (oBranch.className =="doit")
		oBranch.className ="leaf";
}
// This is the dblClick-event handler
function doAll(event) {
	var oBranch, cSubBranches;
	if (event.target) {
		oBranch = event.target;
	} else if (event.srcElement) { // For IE
		oBranch = event.srcElement;
	}
	leaves = oBranch.getElementsByTagName("li");
	if (leaves.length > 0)
		for (i=0, n=leaves.length; i<n; i++)
			if(leaves[i].className =="leaf")
				leaves[i].className ="doit"
			else if(leaves[i].className =="doit")
				leaves[i].className ="leaf";
}
// This function makes nested list items look like links
function addLinksToBranches(oList) {
	var cBranches = oList.getElementsByTagName("li");
	var i, n, cSubBranches;
	if (cBranches.length > 0) {
		for (i=0, n = cBranches.length; i < n; i++) {
			cSubBranches = cBranches[i].getElementsByTagName("ul");
			if (cSubBranches.length > 0) {
				cBranches[i].className = "hcStyle";
				cBranches[i].style.color = "blue";
			}else if((cSubBranches.length == 0)){
				cBranches[i].className = "leaf";
				cBranches[i].style.color = "green";
				par=cBranches[i];
				ttle=par.textContent+"/web";
				par=par.parentNode;
				while(par.className!="LinkedList"){
					if(par.tagName=="UL") ttle="/"+ttle;
					if(par.tagName=="LI"){
						tc=par.textContent;
						tc=tc.substring(0,tc.indexOf("\n"));
						ttle=tc.trim()+ttle;
					} par=par.parentNode;
				} cBranches[i].title=ttle;
			}
		}
	}
}
  </script>
  <style>
.hints:hover {color: black}
.hints {color: lightblue}
img {border: 2px solid #c00;}
</style>
</head><body onload="slide(); addEvents();" >
<h2>Slide Show of <a href="http://pics.tgu.ca">Pics.tgu.ca</a></h2>

