<script type="text/javascript"> <!-- Jelaleddin rotater-->
$(document).ready(function(){
//	picif_start();

//	alert("numero uno");
//	alert(4+" numero zip");
//	alert(5+" numero uno");
//	alert(6+" numero duos "+nID);
//	alert(7+" numero tres "+circuit[nID]+"..."+nSrc);

	FIRST="OFF";
	FIRSTSRC="OFF";
	loop = new Array;
	time=400;
	ANYACTION=1;
	bfr="url('"; aft="')";
	dwell=22;
	inc=1;
	lpl=0;
	stop=1;
	$(".pic>img").each(function(){  //this.height=85;
	});

function run(){
  $('#targ').delay(10*dwell).show(1,function(){
    if(1>stop){
      inc+=1; if(lpl<inc)inc=1;
      FIRST.src=loop[inc-1];
      run();}
  });
}
 $(".r").click(function(event){
    stop=1;
    FIRST.src=FIRSTSRC;
    loop = new Array;
 });


function imageshack2(n,ss) {	var s=ss.split("/");
	return(
		"http://img"+s[0]+".imageshack.us/img"
		+s[0]+"/"+s[1]+"/"+n);	}

$("#full").click(function(event){	// -	-	full click function
		ClicFull=this;
		ClicFullID=fullid;
		ClicFullSRC=this.src;

		for (nID=0; !(ClicFullID==circuit[nID]);  nID++){};

	if(nID==circuit.length-1){nID=0;}else{nID+=1;}
	nSrc=$("#"+circuit[nID]).attr('src');

	if(nSrc.match("imageshack")) {
	} else {
		s=nSrc.split("/");
		nAlt=$("#"+circuit[nID]).attr('alt');
		nAlt=nAlt.split(":");
		nSrc=imageshack2(s[s.length-1],nAlt[1]);
		$("#"+circuit[nID]).attr('src',nSrc);
	}

		this.src=nSrc;
		ClicFullSRC=nSrc;
		fullid=circuit[nID];
/*
*/
		$(".note").append("<strong> full </strong>"
		+ " nID/ " + nID
//		+ "<br>circuit[nID]/ " + circuit[nID]
//		+ "<br>ClicFullID/ " + ClicFullID
		);

});

  $(".pic").click(function(event){	// -	-	pic click function
	$(this).find("img").each(function(){
		ClicPic=this;
		ClicPicSRC=this.src;
		fullid=this.id;
	if(ClicPicSRC.match("imageshack")) {
		constSRC=ClicPicSRC;
		$("#full").attr('src',constSRC);
	} else {
		s=ClicPicSRC.split("/");
//		var slast= s[s.length-1];
		ClicPicALT=this.alt.split(":");
		constSRC=imageshack2(s[s.length-1],ClicPicALT[1]);
		this.src=constSRC;
		$("#full").attr('src',constSRC);

		var parentEls = $(this).parents(".mark")
			.map(function () { return this.tagName+":"+this.className+"/"+this.id; })
			.get().join(", ");
		var ps = $(this).parents(".mark");
		circuit = $(ps[0]).find("div.pic>img")
			.map(function () { return this.id; })
			.get();


//var ibs = $(ps[0]).find("div.pic>img")
//		.map(function () { return this.id; }) .get().join(", ");
//var ibs = $(ps[0]).find("div.pic>img")
//		.map(function () { return this.id+":"+this.parentNode.className; })
//		.get().join(", ");
//var ibs = $(ps[0]).find("div.pic>img") .get();
//var ibs = $(ps[0]).find("img").get();
//var ibs = $(ps[0]).find("img")
//		.map(function () { return this.id; }).get();
//var ibs = $(ps[0]).find("img")
//		.map(function () { return this.id; }).get();

<!-- \q	-	-	-	-	-	-	-	-	-	M O D 1	-	-	-->

//		$(".note").append("<br><strong>" + parentEls+ "</strong>"
//		+" id/ "+fullid);
//		+"<br>ps/ "+ps
//		+"<br>circuit/ "+circuit
//		+"<br>circuit[0]/ "+circuit[0]);
}});});

/*
image
	hier: u/d
	shuf: ar r/f af
	cut plant magnify
thumbs
*/


  $(".l").click(function(event){	// -	-	l click function
    $(this.parentNode).find(".pic:first>img").each(function(){
      this.setAttribute('id','targ');
      FIRST=this;  FIRSTSRC=FIRST.src;  });
    $(this.parentNode).find(".pic>img").each(function(){
      loop.push(this.src);  });
      lpl=loop.length;
      dwell=$("#dwellin").val();
    stop=0;
    run();
   });


  $(".input:text:first").focus();

  $(".note").click(function(event){
//alert("note bene");
	this.innerHTML= "\n<br \>"+this.innerHTML;

	this.innerHTML= "\n<br \>ClicPic= "+ClicPic+this.innerHTML;
	this.innerHTML= "\n<br \>ClicPicSRC= "+ClicPicSRC+this.innerHTML;
	this.innerHTML= "\n<br \>lpl= "+lpl+""+this.innerHTML;
	this.innerHTML= "\n<br \>dwell= "+dwell+""+this.innerHTML;
	this.innerHTML= "\n<br \>FIRSTSRC= "+FIRSTSRC+""+this.innerHTML;
  });
 });
</script>
